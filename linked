        public void add(E e){
            Node newNode = new Node(e);
            if(head == null){
                head == newNode;
                tail == newNode;
            } else{
                tail.next = newNode;
                tail = newNode;
            }
            size++;
        }

        public void remove(){
            if(current == head){ //removing first node
                head = head.next;
                if(head == null){ //empty list
                    tail == null;
                }
            } else{
                Node 
            }
        }
